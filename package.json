{
  "name": "react-use-sub",
  "version": "2.2.1",
  "description": "Subscription based lightweight React store",
  "engines": {
    "node": ">=12"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "index.d.ts",
  "files": [
    "dist/cjs",
    "dist/esm",
    "src",
    "test-util",
    "index.d.ts"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/fdc-viktor-luft/react-use-sub"
  },
  "keywords": [
    "React",
    "TypeScript",
    "Store",
    "useSub"
  ],
  "author": "Viktor Luft <viktor.luft@freiheit.com> (https://github.com/fdc-viktor-luft)",
  "license": "MIT",
  "scripts": {
    "dist": "rm -rf dist && rollup -c && cp index.ts index.d.ts",
    "eslint": "eslint --cache --cache-location 'build/.eslintcache' --ext .ts,.tsx src/ test/",
    "test": "tsc && pnpm eslint && pnpm test-coverage",
    "test-coverage": "jest --all --coverage --colors",
    "coveralls": "cat ./build/coverage/lcov.info | coveralls"
  },
  "peerDependencies": {
    "react": ">= 16.8.0 < 18",
    "react-dom": ">= 16.8.0 < 18"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "babel-jest": "^27.3.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^2.59.0",
    "typescript": "^4.4.4"
  }
}
